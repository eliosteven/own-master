QDialog#LoginDialog, #RegisterDialog{
background-color:rgb(255,255,255)
}

#user_label, #password_label{
    color:black;
}

#err_tip[state='normal']{
    color: green;
}

#err_tip[state='err']{
    color: red;
}

#password_visible[state='unvisible']{
   border-image: url(:/res/unvisible.png);
}

#password_visible[state='unvisible_hover']{
   border-image: url(:/res/unvisible_hover.png);
}

#password_visible[state='visible']{
   border-image: url(:/res/visible.png);
}

#password_visible[state='visible_hover']{
   border-image: url(:/res/visible_hover.png);
}


#password_confirm_visible[state='unvisible']{
   border-image: url(:/res/unvisible.png);
}

#password_confirm_visible[state='unvisible_hover']{
   border-image: url(:/res/unvisible_hover.png);
}

#password_confirm_visible[state='visible']{
   border-image: url(:/res/visible.png);
}

#password_confirm_visible[state='visible_hover']{
   border-image: url(:/res/visible_hover.png);
}

#tip_lb, #tip2_lb{
    color:green;
}

#forget_label[state='normal'],#forget_label[state='selected']{
    color:black;
}

#forget_label[state='hover'], #forget_label[state='selected_hover']{
    color: rgb(42, 112, 241);
}
